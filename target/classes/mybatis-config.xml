<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
 PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-config.dtd">

<!-- mybatis中的全局配置文件各个节点要按照顺序写 -->
<configuration>

	<!-- jdbc.properties文件交给spring记载 -->

	<!-- 设置别名
		package name=""  该包下所有的都会有别名
		别名就是该类的类名,大小写都可以
	 -->
	<typeAliases>
		<package name="com.zhiyou100.model"/>
	</typeAliases>
	
	<!-- 数据源交给spring托管,所以不需要配置数据源 -->
	
	
	<!-- mapper加载交给spring托管 -->
	
	
	
	
</configuration>